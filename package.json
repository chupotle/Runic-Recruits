{
  "name": "runic-recruits",
  "version": "1.0.0",
  "description": "Allow community created leaderboards which can be joined through a room code. This room would contain a leaderboard that lists everyone in the lobby, and then tracks their future games towards that lobby.",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron app.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "Runic.Recruits",
    "mac": {
      "category": "Runic.Recruits"
    },
    "win": {
      "target": "portable",
      "icon": "atom.ico"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chupotle/Runic-Recruits.git"
  },
  "keywords": [
    "LoR",
    "Runic",
    "Recruits",
    "Leaderboards"
  ],
  "author": "Timothy Chu, Imran Gosla",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chupotle/Runic-Recruits/issues"
  },
  "homepage": "https://github.com/chupotle/Runic-Recruits#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "firebase": "^7.4.0",
    "firebase-admin": "^8.8.0"
  },
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-builder": "^21.2.0",
    "electron-packager": "^14.1.1",
    "eslint-config-google": "^0.14.0"
  }
}
